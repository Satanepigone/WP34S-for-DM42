 386:src/stopwatch.c **** 					if(TotalFirstTicker==0) {
 387:src/stopwatch.c **** 						TotalFirstTicker=FirstTicker;
 388:src/stopwatch.c **** 					}
 389:src/stopwatch.c **** 					FirstTicker=getTicker();
