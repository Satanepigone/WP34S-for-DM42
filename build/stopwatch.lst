 391:src/stopwatch.c **** 					TotalFirstTicker=0;
 392:src/stopwatch.c **** 					FirstTicker=0;
 393:src/stopwatch.c **** 				}
 394:src/stopwatch.c **** 				StopWatch=0;
 395:src/stopwatch.c **** 				break;
 396:src/stopwatch.c **** 			   }
