 373:src/stopwatch.c **** static void process_stopwatch_key(int key) {
 374:src/stopwatch.c **** 	int max_registers=global_regs();
 375:src/stopwatch.c **** 	switch(key)	{
 376:src/stopwatch.c **** 			case STOPWATCH_RS: {
